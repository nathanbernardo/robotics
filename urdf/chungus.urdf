<robot name="chungus">
  <link name="base_link" />
  <link name="link2" />
  <link name="link3" />
  <link name="link4" />
  <link name="link5" />

  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link2"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis
      xyz="0 1 0" />
  </joint>
  
  <joint name="joint2" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0.13 0" rpy="0 0 0"/>
    <axis
      xyz="0 0 1" />
  </joint>

  <joint name="joint3" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0 0.24 0" rpy="0 0 0"/>
    <axis
      xyz="0 0 1" />
  </joint>

  <joint name="joint4" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="-0.2 0 0" rpy="0 0 0"/>
    <axis
      xyz="0 0 1" />
  </joint>

</robot>
